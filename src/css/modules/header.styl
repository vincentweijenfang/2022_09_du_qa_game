.header
  position fixed
  position: -webkit-sticky;
  top: 0; /* required */
  left 0
  width 100%
  z-index 201
  // background: linear-gradient(180deg, rgba(black, 0.9) 0%, rgba(black, 0.3) 70%, rgba(black, 0) 99.58%);
  +lg()
    transition:
      background .45s ease;
  .logo
    position relative
    left 0
    transform translateX(0)
    img
      height 80px